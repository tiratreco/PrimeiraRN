#include <stdio.h>
#include "bibliotecas/matriz.h"
#define linhas(m) (sizeof(m) / sizeof(m[0]))
#define colunas(m) (sizeof(m[0]) / sizeof(m[0][0]))

#define ENTRADA 2
#define OCULTO 3
#define SAIDA 1

int redeNeural();

int main (){
    redeNeural();
}

int redeNeural(){
    //cada entrada Ã© uma linha de uma matriz coluna
    Matriz* entradas = m_construct(ENTRADA, 1, true);

    //pesos de uma entrada ficam organizados numa mesma coluna
    Matriz* pesos = m_construct(ENTRADA, OCULTO, true);

    m_imprimir(entradas);
    m_imprimir(pesos);

    Matriz* respostas = m_multiplicar(entradas, pesos);
    m_imprimir(respostas);

}
